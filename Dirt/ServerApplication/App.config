    <add key="MaxClient" value="12" />
    <add key="PluginFile" value="CustomPlugin.dll" />
    <add key="PluginClass" value="CustomPluginNS.PluginClass" />
    <add key="ContentRoot" value="../../../../Content/" />
    <add key="PersistentRoot" value="."/>
